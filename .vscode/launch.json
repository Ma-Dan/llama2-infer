    "version": "0.2.0",
    "configurations": [
        {
            "name": "export_model",
            "type": "debugpy",
            "request": "launch",
            "program": "export_model.py",
            "args": ["/root/autodl-tmp/Qwen1.5-1.8B-Chat", "qwen_chat_18b"],
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {}
        },
        {
            "name": "inference",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/inference",
            "args": ["qwen_chat", "", "4096", "0.85"],
            //"args": ["stories42M", "", "256", "0"],
            "environment": [{"name": "OMP_NUM_THREADS", "value": "8"}],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                { "text": "-enable-pretty-printing", "description": "enable pretty printing", "ignoreFailures": true },
                { "text": "handle SIGPIPE nostop noprint pass", "description": "ignore SIGPIPE", "ignoreFailures": true }
            ]
        },
        {
            "name": "gen_test",
            "type": "debugpy",
            "request": "launch",
            "program": "gen_test.py",
            "args": [],
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {}
        },
    ]
